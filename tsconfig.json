{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "moduleResolution": "node",
        "noLib": false,
        "module": "commonjs",
        "experimentalDecorators": true,
        "lib": [
            "es5",
            "dom",
            "es2015.core",
            "es2015.promise"
        ],
        "preserveConstEnums": false,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "jsx": "react",
        "reactNamespace": "b"
    },
    "filesGlob": [],
    "files": [],
    "include": [
        "**/*"
    ]
}